{
  "name": "podcasts-player",
  "version": "0.0.1",
  "description": "Podcasts player on web technologies",
  "main": "index.js",
  "author": "Pavel Grinchenko <psdcoder@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "NODE_ENV=production webpack --config webpack.production.js",
    "start": "NODE_ENV=development webpack-dev-server --config webpack.development.js --hot --open",
    "test": "jest --maxWorkers=4",
    "lint": "yarn run lint:js",
    "lint:js": "eslint . --ext .js --ext .jsx",
    "lint:js:fix": "eslint . --fix",
    "lint:css": "stylelint './src/**/*.jsx'",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.pcss": "stylelint",
    "*.{js,jsx}": "eslint --quiet"
  },
  "devDependencies": {
    "@babel/core": "7.0.0-beta.40",
    "@babel/plugin-proposal-class-properties": "7.0.0-beta.40",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0-beta.40",
    "@babel/plugin-transform-destructuring": "7.0.0-beta.40",
    "@babel/plugin-transform-react-display-name": "7.0.0-beta.40",
    "@babel/plugin-transform-react-jsx": "7.0.0-beta.40",
    "@babel/preset-env": "7.0.0-beta.40",
    "@babel/preset-react": "7.0.0-beta.40",
    "babel-eslint": "8.2.2",
    "babel-loader": "8.0.0-beta.2",
    "babel-plugin-emotion": "9.0.1",
    "babel-plugin-transform-react-remove-prop-types": "0.4.13",
    "clean-webpack-plugin": "0.1.18",
    "eslint": "4.18.2",
    "eslint-config-airbnb": "16.1.0",
    "eslint-import-resolver-webpack": "0.8.4",
    "eslint-plugin-import": "2.9.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.7.0",
    "file-loader": "1.1.11",
    "html-webpack-plugin": "3.0.4",
    "husky": "0.14.3",
    "lint-staged": "7.0.0",
    "react-hot-loader": "4.0.0",
    "stylelint": "9.1.1",
    "stylelint-config-standard": "18.2.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.3.0",
    "svg-url-loader": "2.3.1",
    "url-loader": "1.0.1",
    "webpack": "4.1.0",
    "webpack-cli": "2.0.10",
    "webpack-dev-server": "3.1.0"
  },
  "dependencies": {
    "emotion": "9.0.2",
    "prop-types": "15.6.1",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-emotion": "9.0.2",
    "react-redux": "5.0.7",
    "redux": "3.7.2"
  }
}
